<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!--Mã Adapter-->
        <record id="seq_adapter_code" model="ir.sequence">
            <field name="name">Adapter Code</field>
            <field name="code">adapter_code</field>
            <field name="prefix">ADT%(day)s%(month)s%(year)s</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <!--Mã Mắt-->
        <record id="seq_mat_code" model="ir.sequence">
            <field name="name">Mắt Code</field>
