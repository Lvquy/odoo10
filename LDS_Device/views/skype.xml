<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <!--tree view-->
        <record id="view_skype_tree" model="ir.ui.view">
            <field name="name">skype.tree</field>
            <field name="model">skype</field>
            <field name="priority" eval="8" />
            <field name="arch" type="xml">
                <tree string="skype">
                    <field name="name"/>
                    <field name="passw"/>
                    <field name="nguoi_sd"/>
                </tree>
            </field>
        </record>

        <!--form view-->
        <record id="view_skype_form" model="ir.ui.view">
            <field name="name">skype.form</field>
            <field name="model">skype</field>
            <field name="priority" eval="8" />
            <field name="arch" type="xml">
                <form string="Skype">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="passw"/>
                            <field name="nguoi_sd" options="{'no_create': True}"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>