<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!--Mã đơn hàng-->
        <record id="seq_khach_code" model="ir.sequence">
            <field name="name">Mã khách</field>
            <field name="code">khach_code</field>
            <field name="prefix">KH%(month)s%(year)s</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <!--invoice_code-->
        <record id="seq_invoice_code" model="ir.sequence">
            <field name="name">Mã hóa đơn</field>
            <field name="code">invoice_code</field>
            <field name="prefix">INV%(month)s%(year)s</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <!--line_invoice_code-->
        <record id="seq_line_invoice_code" model="ir.sequence">
            <field name="name">Mã hóa đơn</field>
            <field name="code">line_invoice_code</field>
            <field name="prefix">LINV%(month)s%(year)s</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
        <!--ma giao dich nap tien-->
        <record id="seq_naptien_code" model="ir.sequence">
            <field name="name">Mã giao dịch nạp tiền</field>
            <field name="code">naptien_code</field>
            <field name="prefix">NT%(day)s%(month)s</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>